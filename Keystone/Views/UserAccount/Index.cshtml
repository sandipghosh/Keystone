@using Keystone.Web.Models
@model UserAccountModel

@if (ViewBag.ViewType.ToString() == "ChangePasswordForNewUser" ||
    ViewBag.ViewType.ToString() == "ChangePassword")
{
    @Html.Partial("_ChangePassword", Model, new ViewDataDictionary {
        { "ViewType", ViewBag.ViewType.ToString() },
        { "ReferalUrl", ((object)ViewBag.ReferalUrl ?? "") }
    });
}
else if (ViewBag.ViewType.ToString() == "SignIn")
{
    @Html.Partial("_SignInPartial", Model, new ViewDataDictionary {
        { "ReferalUrl", ((object)ViewBag.ReferalUrl ?? "") }
    });
}
else if (ViewBag.ViewType.ToString() == "ChangeProfile")
{
    @Html.Partial("_ChangeProfile", Model, new ViewDataDictionary {
        { "ReferalUrl", ((object)ViewBag.ReferalUrl ?? "") }
    });
}
else if (ViewBag.ViewType.ToString() == "ForgotPassword")
{
    @Html.Partial("_ForgotPassword", Model, new ViewDataDictionary {
        { "ReferalUrl", ((object)ViewBag.ReferalUrl ?? "") }
    });
}